.container
	display flex
	flex-wrap wrap
	gap .1rem
	padding 1rem
	border-radius 0.5rem
.cell	
	background-color trasparent
	padding .1rem
	flex-grow 1
	border-radius radius
	font-size  0.5rem
	color azure
	
.container-two
	display grid
	grid-template-columns  1fr 1fr
	grid-template-rows 1fr
	padding-top  1.5em
.on-left
	grid-row 1
	grid-column 1
	
.on-right
	grid-row 1
	grid-column 2
	padding 1em	
	text-align center		
		