{
  "name": "parcel-vue-template",
  "version": "1.0.10",
  "description": " Boilerplate Vue3 + Parcel for fast start and exploring capability.",
  "source": "[src/*.html]",
   "scripts": {
    "start": "parcel src/*.pug src/market.pug -p 7778 --no-cache --host localhost",
    "build": "parcel build src/*.pug src/market.pug --public-url /parcel-vue-template/ --dist-dir docs ",
   
  },
  "keywords": [],
  "author": " Aleksey Bazhenov todhruva@mail.ru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dhruuva/parcel-vue-template/issues"
  },
  "alias": {
    "vue": "vue/dist/vue.esm-browser.prod"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhruuva/parcel-vue-template.git"
  },
  "@parcel/transformer-css": {
    "cssModules": false,
    "dashedIdents": true,
    "pattern": "[hash]-[local]"
    
  },

  "devDependencies": {
    "@parcel/compressor-brotli": "^2.5.0",
    "@parcel/compressor-gzip": "^2.5.0",
    "@parcel/transformer-pug": "^2.5.0",
    "@parcel/transformer-stylus": "^2.5.0",
    "@parcel/transformer-vue": "^2.5.0",
    "@vue/compiler-sfc":"latest",
    "parcel": "^2.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@iconify/vue": "^3.2.1",
    "vue": "^3.2.33"
  }
}
