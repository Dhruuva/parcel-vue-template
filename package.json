{
  "name": "parcel-vue-template",
  "version": "1.0.11",
  "description": " Boilerplate Vue3 + Parcel for fast start and exploring capability.",
  "source": "[src/*.html]",
  "scripts": {
    "watch": "rm -rf dist && parcel src/*.pug --no-source-maps -p 7778",
    "start": "rm -rf dist && parcel src/*.pug -p 7778 --host localhost",
    "serve": "rm -rf dist parcel src/*.pug -p 7778 --no-cache --host localhost --no-source-maps",
    "build": "parcel build src/*.pug --no-source-maps --public-url /parcel-vue-template/ --dist-dir docs",
    "clean": "rm -rf dist .parcel-cache"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "keywords": [
    "parcel"
  ],
  "author": " Aleksey Bazhenov todhruva@mail.ru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dhruuva/parcel-vue-template/issues"
  },
  "alias": {
    "vue": "vue/dist/vue.esm-browser.prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhruuva/parcel-vue-template.git"
  },
  "@parcel/transformer-css": {
    "cssModules": false,
    "dashedIdents": true,
    "pattern": "[hash]-[local]"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "latest",
    "@parcel/transformer-stylus": "latest",
    "@parcel/transformer-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "parcel": "latest"
  },
  "dependencies": {
    "@iconify/vue": "^3.2.1",
    "vue": "latest"
  }
}
